#!/usr/bin/env python3
import argparse


p = argparse.ArgumentParser(description = "Plot a mass spectrum.")

p.add_argument("-s", type=str,
               help="Path to the input ASCI/mzML/mzXML file.")
p.add_argument("--t", type=str, default=".",
               help="Path to the output html file.")
args = p.parse_args()



